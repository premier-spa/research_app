<%= form_for(resource, as: resource_name, url: registration_path(resource_name), local: true) do |f| %>
  <%= devise_error_messages! %>
  <div class="row" id="professor_info">
    <div class="col s12">
      <div class="card col s12">
        <div class="input-field col s12">
          <i class="material-icons prefix">event_seat
</i>
          <%= f.label :position %>
          <%= f.text_field :position %>
        </div>

        <div class="input-field col s12">
          <i class="material-icons prefix">business_center</i>
          <%= f.label :career %>
          <%= f.text_area :career, class: "materialize-textarea" %>
        </div>
      </div>
    </div>
  </div>
<% end %>
