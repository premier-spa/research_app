<h1>お問い合わせはこちら！</h1>

<div id="sample" style="width: 700px; height: 400px;"></div>

<script>
var map;
function initMap() {
    map = new google.maps.Map(document.getElementById('sample'), { // #sampleに地図を埋め込む
        center: { // 地図の中心を指定
            lat: 34.7019399, // 緯度
            lng: 135.51002519999997 // 経度
        },
        zoom: 19 // 地図のズームを指定
    });
}
</script>
<script async defer src="<%= @map_script_path %>" type="text/javascript"></script>